{
	"name": "byjg/xmlnuke",
	"description": "PHP Framework based on XML/JSON",
	"type": "project",
	"keywords": ["api", "framework", "xml", "json", "xsl", "xmlnuke"],
	"homepage": "http://xmlnuke.com",
	"license": "GPL",
	"authors": [
		{
			"name": "João Gilberto Magalhães",
			"email": "joao@byjg.com.br"
		}
	],
	"config": {
		"vendor-dir": "xmlnuke-php5/src/vendor"
	},
	"bin": [
		"copy-dist-files.sh",
		"create-php5-project.php",
		"update-php5-project.php"
	],
	"support": {
		"issues": "https://github.com/byjg/xmlnuke/issues",
		"source": "https://github.com/byjg/xmlnuke"
	},
	"require": {
		"php": ">=5.4.0",
		"ext-json": "*",
		"ext-dom": "*",
		"ext-xml": "*",
		"ext-xsl": "*",
		"ext-session": "*",
		"apache/log4php": "2.3.0",
		"phpmailer/phpmailer": "5.2.9",
		"filp/whoops": "1.1.2",
		"neitanod/forceutf8": "dev-master",
		"mobiledetect/mobiledetectlib": "2.8.5",
		"willdurand/negotiation": "1.3.4",
		"nikic/fast-route": "dev-master",
		"byjg/authuser": "dev-master",
		"byjg/imageutil": "1.0.*",
		"byjg/singleton-pattern": "1.0.*",
		"byjg/webrequest": "1.0.*", 
        "byjg/anydataset": "1.0.*",
        "byjg/cache-engine": "1.0.*",
        "byjg/convert": "1.0.*",
        "byjg/mailwrapper": "1.0.*",
        "byjg/xmlutil": "1.0.*",
        "sheikhheera/iconfig": "dev-master"
	},
	"suggest": {
		"ext-curl": "*",
		"ext-pdo_mysql": "*"
	},
	"autoload": {
		"psr-0": {
			"Xmlnuke\\": "xmlnuke-php5/src/"
		}
	}
}
